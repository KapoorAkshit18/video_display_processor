// executer by Akshit 

module execute (                // terminals
  input [15:0] a,
  input [15:0] b,
  input add,
  output [15:0] result
);
  wire [15:0] s0;             // carries sum 
  adder_nbits #(
    .bits(16)
  )
  adder_nbits_i0 (
    .a( a ),
    .b( b ),
    .c_i( 1'b0 ),
    .s( s0 )
  );
  mux_2x1_nbits #(
    .bits(16)
  )
  mux_2x1_nbits_i0 (
    .sel( add ),
    .xin_0( b ),
    .xin_1( s0 ),
    .xout( result )
  );
endmodule







